  "scripts": {
    "format-all": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "build_and_serve": "yarn build; yarn serve",
    "dev": "node --loader ts-node/esm --experimental-specifier-resolution=node src/server.ts",
    "build": "craco build && tsc -p tsconfig.backend.json",
    "test-ci": "CI=true craco build",
    "test": "jest",
    "serve": "NODE_ENV=production node node_dist/server.js"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "\\.(css|less|svg)$": "<rootDir>/src/mockStyle.js",
      "nav-(.*)-style": "<rootDir>/src/mockStyle.js"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.json"
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest-setup.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "eslint --fix src/ --max-warnings=0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
