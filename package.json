{
  "name": "familie-ba-soknad",
  "type": "module",
  "scripts": {
    "format-all": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "build_and_serve": "yarn build; yarn start",
    "start:dev": "BASE_PATH=/familie/barnetrygd/soknad/ordinaer/ yarn clean && concurrently \"npm:dev:*\"",
    "start:dev:ts-server": "BASE_PATH=/familie/barnetrygd/soknad/ordinaer/ yarn clean && concurrently --restart-tries \"-1\" \"npm:dev:webpack\" \"npm:ts:server\"",
    "dev:server": "tsc -p tsconfig.backend.json && NODE_ENV=development node --es-module-specifier-resolution=node build/backend/server.js",
    "ts:server": "NODE_ENV=development TS_NODE_PROJECT=tsconfig.backend.json nodemon --exec 'node' --watch src/backend -e ts --es-module-specifier-resolution=node --loader ts-node/esm src/backend/server.ts",
    "dev:webpack": "TS_NODE_PROJECT=src/webpack/tsconfig.json NODE_ENV=development webpack -c src/webpack/webpack.development.config.ts",
    "build": "TS_NODE_PROJECT=src/webpack/tsconfig.json NODE_ENV=production webpack --config src/webpack/webpack.production.config.ts && tsc -p tsconfig.backend.json",
    "build:analysis": "TS_NODE_PROJECT=src/webpack/tsconfig.json NODE_ENV=production webpack --config src/webpack/webpack.analysis.config.ts",
    "test-ci": "CI=true build",
    "test": "jest",
    "start": "NODE_ENV=production node --es-module-specifier-resolution=node build/backend/server.js",
    "clean": "rm -rf dist build",
    "typecheck": "tsc --noEmit -p src/frontend/tsconfig.json",
    "setup:docker:env": "node dockerstuff/dcp-configurator.js"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest-setup.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/mockStyle.js",
      "nav-(.*)-style": "<rootDir>/src/mockStyle.js",
      "nav-frontend-core": "<rootDir>/src/mockStyle.js",
      "styled-components/macro": "styled-components"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@navikt)/)",
      "\\.pnp\\.[^\\/]+$"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      "\\.svg$": "jest-svg-transformer"
    },
    "resetMocks": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn run typecheck"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "eslint --fix src/ --max-warnings=0"
    ]
  },
  "resolutions": {
    "styled-components": "^5",
    "browserslist": "^4.16.5",
    "ansi-regex": "^5.0.1",
    "webpack-plugin-ramdisk": "^0.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
